{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/health",
            "dest": "/api/health.js"
        },
        {
            "src": "/api/extract-pdf",
            "dest": "/api/extract-pdf.js"
        },
        {
            "src": "/api/chat",
            "dest": "/api/chat.js"
        },
        {
            "src": "/api/generate-html",
            "dest": "/api/generate-html.js"
        },
        {
            "src": "/api/generate-app",
            "dest": "/api/generate-app.js"
        }
    ]
}